@model IEnumerable<elearning_b1.Models.Topic>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Topics</h1>
<hr />
<div class="list-group">
    @foreach (var topic in Model)
    {
        <a href="@Url.Action("VocabularyList", "Vocabularies", new { topicId = topic.TopicID })" class="list-group-item list-group-item-action">
            @topic.TopicName
        </a>
    }
</div>